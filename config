[Buttons]
1: drag
8: prev
9: next

[Keys]
S-space: prev
space: next

H: pageLeft
J: pageDown
K: pageUp
L: pageRight

h: left
j: down
k: up
l: right

W: pageUp
A: pageLeft
S: pageDown
D: pageRight

w: up
a: left
s: down
d: right

G: scrollEnd
g: scrollStart

R: rotateL
r: rotateR
t: slower
T: faster

b: status
e: slideshow
f: fullscreen
q: quit
#x: delete
z: zoom
c: zoomIn
v: zoomOut
C-z: toggle_auto_zoom

[Options]
format: {i}/{l} {path} {slide}
#fullscreen: true
slideshow delay: 5
auto zoom: true